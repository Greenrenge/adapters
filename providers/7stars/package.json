{
  "name": "@kkg/7stars",
  "version": "1.0.0",
  "description": "template for any libraries",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf ./dist && babel src -s -d dist --source-maps",
    "test": "cross-env NODE_ENV=test mocha --recursive \"test/**/*.test.js\"",
    "lint": "eslint --ignore-path .eslintignore .",
    "lint:fix": "npm run lint -- --fix",
    "test:watch": "cross-env NODE_ENV=test nodemon --watch test --watch src --exec npm run test",
    "start:watch": "cross-env NODE_ENV=development nodemon --watch src --exec node_modules/.bin/babel-node -- src/index.js",
    "start": "cross-env NODE_ENV=development nodemon --watch --exec node_modules/.bin/babel-node -- src/index.js",
    "demo": "cross-env NODE_ENV=development nodemon --watch --exec node_modules/.bin/babel-node -- demo/index.js"
  },
  "author": "Kitthanat S.",
  "license": "ISC",
  "publishConfig": {
    "directory": "dist"
  },
  "dependencies": {
    "@greenrenge/request": "^1.0.3",
    "@kkg/async-packs": "file:../../packages/async-packs",
    "@kkg/captcha-packs": "file:../../packages/captcha-packs",
    "@kkg/demo": "file:../../packages/demo",
    "@kkg/redis-packs": "file:../../packages/redis-packs",
    "cheerio": "^1.0.0-rc.3",
    "debug": "^4.2.0",
    "lodash": "^4.17.20",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  }
}
